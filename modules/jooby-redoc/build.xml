<?xml version="1.0"?>
<project default="redoc">

  <target name="redoc">
    <property name="src" value="${basedir}${file.separator}src${file.separator}main${file.separator}resources" />
    <property name="output" value="${src}${file.separator}redoc" />
    <mkdir dir="${output}" />
    <get src="https://cdn.jsdelivr.net/npm/redoc@latest/bundles/redoc.standalone.js" usetimestamp="true"
         dest="${output}"/>
  </target>

</project>
