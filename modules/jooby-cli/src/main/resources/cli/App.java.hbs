package {{package}};

import io.jooby.Jooby;
{{#if openapi}}
import io.jooby.OpenAPIModule;
{{/if}}

public class App extends Jooby {

  {
    {{#if openapi}}
    install(new OpenAPIModule());
    {{/if}}

    {{#if mvc}}
    mvc(new Controller());
    {{else}}
    get("/", ctx -> "Welcome to Jooby!");
    {{/if}}
  }

  public static void main(final String[] args) {
    runApp(args, App::new);
  }

}
